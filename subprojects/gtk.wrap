[wrap-git]
directory = gtk
url = https://gitlab.gnome.org/GNOME/gtk.git
push-url = ssh://git@ssh.gitlab.gnome.org:GNOME/gtk.git
revision = main
depth = 1

[provide]
dependency_names = gtk4, gtk4-wayland, gtk4-x11
program_names = gtk4-update-icon-cache, gtk4-builder-tool, gtk4-query-settings, gtk4-encode-symbolic-svg
