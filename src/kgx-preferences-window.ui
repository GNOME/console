<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="KgxPreferencesWindow" parent="AdwPreferencesWindow">
    <child>
      <object class="AdwPreferencesPage">
        <property name="title" translatable="yes">General</property>
        <child>
          <object class="AdwPreferencesGroup">
            <property name="title" translatable="yes">Sounds</property>
            <child>
              <object class="AdwSwitchRow" id="audible_bell">
                <property name="title" translatable="yes">_Terminal Bell</property>
                <property name="use-underline">True</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
  <object class="GBindingGroup" id="settings_binds">
    <property name="source" bind-source="KgxPreferencesWindow" bind-property="settings" bind-flags="sync-create"/>
  </object>
</interface>
